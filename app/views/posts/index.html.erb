<h3>Lista wpisów</h3>

<table class="table table-striped">
  <thead>
    <tr>
      <th>#</th>
      <th>Tytuł</th>
      <th>Autor</th>
      <th>Opublikowany?</th>
      <th>Treść</th>
      <th>Akcje</th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.id %></td>
        <td><%= post.title %></td>
        <td><%= post.author %></td>
        <td><%= post.published ? "tak" : "nie" %></td>
        <td><%= post.body %></td>
        <td><%= %></td>
      </tr>
    <% end %>
  </tbody>
</table>
